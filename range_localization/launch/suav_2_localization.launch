<launch>

    <node pkg="rtls_uwb_sensor" type="display" name="suav_2_uwb_node" output="log" >
       <remap from="/uwb/data" to="/suav_2/uwb/data" />
    </node>

    <node pkg="range_localization" type="range_node" name="suav_2_range_node" output="screen" >
        <rosparam command="load" file="$(find range_localization)/config/uav.yaml" />
        <param name="vehicle_name"  value="suav_2" />
        <param name="imu_topic" value="/suav_2/imu/data" />
        <param name="range_topic"  value="/suav_2/uwb/data" />
        <param name="pressure_topic"  value="/suav_2/dji_osdk_ros/height_above_takeoff" />
        <rosparam param="init_position" >[0.0, 0.0, 0.50]</rosparam>
    </node>
        
    <!-- <node pkg="rosbag" type="record" name="suav_2_bag_record" args="/suav_2/uwb/data /suav_2/imu/data /suav_2/filter/odom /suav_2/dji_osdk_ros/height_above_takeoff /suav_2/dji_osdk_ros/vo_position /suav_2/dji_osdk_ros/imu -o /home/$(env USER)/suav_2.bag"/> -->
</launch>

